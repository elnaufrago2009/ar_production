<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Parte Diario de Obras - Bagua 2013</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">    
    
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag 'jquery' %>
    <%= javascript_include_tag 'jquery_ujs' %>
    <%= javascript_include_tag 'bootstrap-dropdown' %>
    <%= javascript_include_tag 'bootstrap-button' %>
    <%= javascript_include_tag 'bootstrap-collapse' %>
    <%= javascript_include_tag 'bootstrap-tooltip' %>
    <%= javascript_include_tag 'partidasdecontrols' %>
    <%= javascript_include_tag 'bootstrap-datepicker/core' %>
    <%= javascript_include_tag 'bootstrap-datepicker/locales/bootstrap-datepicker.es' %>
    <%= javascript_include_tag 'select2' %>
    <%= javascript_include_tag 'jquery.validate.min' %>   
    <%= javascript_include_tag 'additional-methods' %>


    <!-- Meta Tags 
    ============================-->
  	<%= csrf_meta_tags %>


    <!-- Compatibilidad con Navegadores 
    =================================================================    -->
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->


  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <%= link_to "ARSAC - Produccion", root_path , :class => "brand" %>
          <div class="nav-collapse collapse">
            <ul class="nav">

              <!-- General
              ============================================    -->
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">General <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li class="nav-header">Principal</li>                    
                    <li><%= link_to raw('<i class="icon-th"></i> Entes'), entes_path %></li>
                    <li><%= link_to raw("<i class='icon-calendar'></i> Calendario"), anhos_path%></li>
                    <li><%= link_to raw('<i class="icon-list-alt"></i> Proveedores'), proveedores_path %></li>
                    <!-- Partidas Importacion -->
                    
                    <li class="dropdown-submenu">
                    <%= link_to raw('<i class="icon-th"></i> Importar Partidas'), "/listas/import", :tabindex => '-1'
                     %>                   
                      <ul class="dropdown-menu">
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Partidas'), listas_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Categoria Partidas'), listacategorias_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Importar'), "/listas/import" %></li>
                      </ul>
                    </li> 

                    <!-- Grupos de trabajo -->
                    <li class="divider"></li>
                    <li class="nav-header">Grupos de Trabajo</li>
                    <li class="dropdown-submenu">
                    <%= link_to raw('<i class="icon-th"></i> Grupos de Trabajo'), gruposdetrabajos_path, :tabindex => '-1'
                     %>                   
                      <ul class="dropdown-menu">
                        <li><%= link_to raw('<i class="icon-share-alt"></i> WBS nivel 1'), sectores_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> WBS nivel 2'), subsectores_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Frente'), frentes_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Jefes de Frentes'), jefedefrentes_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Maestro de Obras'), maestrodeobras_path %></li>
                      </ul>
                    </li>                    
                    <!-- Partidas de Control -->
                    <li class="divider"></li>
                    <li class="nav-header">Partidas de Control</li>
                    <li class="dropdown-submenu">
                    <%= link_to raw('<i class="icon-th"></i> Partidas de Control'), partidasdecontrols_path, :tabindex => '-1' %>
                      <ul class="dropdown-menu">
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Sectores'), sectores_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Sub Sectores'), subsectores_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Fases'), fases_path %></li>
                        <li><%= link_to raw('<i class="icon-share-alt"></i> Sub Fases'), subfases_path %></li>
                      </ul>
                    </li>
                    <li class="divider"></li>
                    <li class="nav-header">Unidades</li>
                    <li><%= link_to raw('<i class="icon-indent-right"></i> Unidades'), unidades_path %></li>
                    <li class="divider"></li>
                    <li class="nav-header">Subcontratos</li>
                    <li class="dropdown-submenu">
                        <%= link_to raw('<i class="icon-th"></i> Subcontratos'), subcontratos_path, :tabindex => '-1' %>
                          <ul class="dropdown-menu">
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Insumos de Subcontratos'), insumos_path %></li>
                            <li><%= link_to raw('<i class="icon-share-alt"></i>Catalogo Insumos de contratos'), listas_path %></li>
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Categoria Insumos contratos'), listacategorias_path %></li>
                          </ul>
                    </li>
                    <li class="dropdown-submenu">
                        <%= link_to raw('<i class="icon-th"></i> Subcontratos de Equipos'), subcontratodeequipos_path, :tabindex => '-1' %>
                          <ul class="dropdown-menu">
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Insumos de Equipos'), insumodeequipos_path %></li>
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Equipos de Subcontratos'), equiposdesubcontratos_path %></li>
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Equipo Incluyes'), equipoincluyes_path %></li>
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Tipos de Combustible'), combustibletipos_path %></li>
                          </ul>
                    </li>
                    <li class="divider"></li>
                    <li class="nav-header">Trabajadores</li>
                    <li class="dropdown-submenu">
                        <%= link_to raw('<i class="icon-th"></i> Catalogo de Trabajadores'), catalogodetrabajadores_path, :tabindex => '-1' %>
                          <ul class="dropdown-menu">
                            <li><%= link_to raw('<i class="icon-share-alt"></i> Tipos de Trabajadores'), catalogocategoriatrabajadores_path %></li>
                          </ul>
                    </li>
                  </ul>
              </li>
              
              <!-- Parte de Obras
              ============================================    -->
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Produccion <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li class="nav-header">Parte Obra : Principal</li>
                      <li><%= link_to raw('<i class="icon-th"></i> Lista de Parte Obras'), parteobras_path %></li>                      
                    <li class="divider"></li>
                      <li><%= link_to raw('<i class="icon-th"></i> Partidas'), partidas_path %></li>
                  </ul>
              </li>

              <!-- Parte Diario de Personal 
              ==============================================    -->
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Personal <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li class="nav-header">Parte : Principal</li>
                  <li><%= link_to raw('<i class="icon-th"></i> Parte de Personal'), partediariodepersonals_path %></li>
                  <li class="divider"></li>
                  <li class="nav-header">Secciones Trabajadores</li>
                  <li><%= link_to raw('<i class="icon-list"></i> Trabajadores Subidos'), trabajadores_path %></li>
                  <li><%= link_to raw("<i class='icon-th'></i> Tareo de Obreros"), "/tareos/tareo_subcontratista" %></li>
                  <li><%= link_to raw("<i class='icon-th'></i> Tareo Semanal"), "/tareos/tareo_semanal" %></li>
                </ul>
              </li>


              <!-- Sub Contrato de Equipos
              ============================================    -->
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Equipos <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li class="nav-header">Principal</li>
                    <li><%= link_to raw("<i class='icon-th'></i> Parte de Equipos"), partedeequipos_path %></li>
                    <li><%= link_to raw("<i class='icon-th'></i> Equipos Registrados"), equiposregistrados_path %></li>
                  </ul>
              </li>

              <!-- Consumo Agregados
              ============================================    -->
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Agregados <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li class="nav-header">Principal</li>
                    <li><%= link_to raw('<i class="icon-th"></i> Vales de Consumo'), valesdeconsumos_path %></li>
                  </ul>
              </li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Valorizaciones <b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li class="nav-header">Produccion</li>
                    <li><%= link_to raw('<i class="icon-th"></i> Analisis de Produccion'), "/valorizaciones/produccion_fecha" %></li>
                    <li><%= link_to raw('<i class="icon-th"></i> Graficos Analisis Produccion'), '/apgraficos/start' %></li>
                    <li><%= link_to raw("<i class='icon-th'></i> Valorizaciones SC"), "/valorizaciones" %></li>
                    <li class="nav-header">Equipos</li>
                    <li><%= link_to raw("<i class='icon-th'></i> Reporte de Equipos"), "/reportedeequipos/primero" %></li>
                    <li><%= link_to raw("<i class='icon-th'></i> Reporte de Maquinaria"), "/reportedemaquinarias/primero" %></li>
                    <li><%= link_to raw("<i class='icon-th'></i> Valorizar Equipos"), reportedemaquinarias_path %></li>
                    
                  </ul>
              </li>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">

      <%= yield %>

      <hr>
      <footer>
        <p>&copy; Arsac 2013</p>
      </footer>

    </div> <!-- /container -->
    
    
  </body>
</html>
