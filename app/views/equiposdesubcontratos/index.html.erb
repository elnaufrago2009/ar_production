    <div class="page-header">
    <h1>Equipos Registrados</h1>    
</div>


<table class="table table-bordered table-condensed table-striped table-hover">
  <tr>
    <th>#id</th>
    <th>Subcontrato</th>
    <th>Codigo</th>    
    <th>Insumo de equipos</th>           
    <th>Und</th>
    <th>P. sin igv</th>    
    <th>Hrs Min</th>
    <th>Acciones</th>
  </tr>

<% @equiposdesubcontratos.each do |equiposdesubcontrato| %>
  <tr style="font-size: 11px;">
    <td><%= equiposdesubcontrato.id %></td>
    <td>
        <%= equiposdesubcontrato.subcontratodeequipo.proveedore.ente.nombre[0,15] %>
        <%= equiposdesubcontrato.subcontratodeequipo.proveedore.ente.apellido[0,10] %>
        <%= equiposdesubcontrato.subcontratodeequipo.proveedore.ente.razon_social[0,22] %>
    </td>
    <td><%= equiposdesubcontrato.codigo %></td>    
    <td><%= equiposdesubcontrato.insumodeequipo.nombre %></td>
    <td><%= equiposdesubcontrato.unidade.nombre %></td>
    <td><%= equiposdesubcontrato.precion_sin_igv %></td>
    <td><%= equiposdesubcontrato.cantidad_h_m %></td>
    <td>
        <%= link_to raw('<i class="icon-eye-open icon-white"></i>'), equiposdesubcontrato, :class => 'btn btn-small btn-info' %>
        <%= link_to raw('<i class="icon-edit icon-white"></i>'), edit_subcontratodeequipo_path(equiposdesubcontrato.subcontratodeequipo_id), :class => 'btn btn-small btn-success' %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to raw('<i class="icon-arrow-left icon-white"></i> regresar'), subcontratodeequipos_path, :class => 'btn btn-primary' %>
