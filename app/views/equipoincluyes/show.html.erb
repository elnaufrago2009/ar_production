<p id="notice"><%= notice %></p>

<div class="page-header">
	<h2>Ver Incluye</h2>
</div>

<div class="row">
<table class="table table-striped table-condensed table-bordered span5">
	<tr>
		<th>Nombre</th>
		<th>Descripcion</th>
	</tr>
	<tr>
		<td>#id</td>
		<td><%= @equipoincluye.id %></td>
	</tr>
	<tr>
		<td>Nombre</td>
		<td><%= @equipoincluye.nombre %></td>
	</tr>
</table>
</div>

<div class="clearfix"></div>


<div class="form-actions">
	<%= link_to raw('<i class="icon-edit icon-white"></i> edit'), edit_equipoincluye_path(@equipoincluye), :class => 'btn btn-success' %>
	<%= link_to raw('<i class="icon-arrow-left icon-white"></i> Regresar'), equipoincluyes_path, :class => 'btn btn-primary' %>	
</div>

