<% if !notice.nil? %>
  <div class="alert alert-success">
    <%= notice %>
  </div>
<% end %>


<h2>Parte de Obra <small><%= @parteobra.fecha %></small></h2>

<table class="table table-striped table-condensed table-bordered" >
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
  </tr>
  <tr>
    <td>#id</td>
    <td><%= @parteobra.id %></td>
  </tr>
  <tr>
    <td>Numero de Parte</td>
    <td><%= @parteobra.numero %></td>
  </tr>
  <tr>
    <td>Jefe de Frente</td>
    <td>
      <%= @parteobra.gruposdetrabajo.jefedefrente.ente.nombre %> 
      <%= @parteobra.gruposdetrabajo.jefedefrente.ente.apellido %> 
      <%= @parteobra.gruposdetrabajo.jefedefrente.ente.razon_social %>
    </td>
  </tr>
  <tr>
    <td>Subcontratista</td>
    <td>
      <%= @parteobra.gruposdetrabajo.subcontrato.proveedore.ente.nombre %>
      <%= @parteobra.gruposdetrabajo.subcontrato.proveedore.ente.apellido %> 
      <%= @parteobra.gruposdetrabajo.subcontrato.proveedore.ente.razon_social %>
    </td>
  </tr>
  <tr>
    <td>WBS Nivel 1</td>
    <td><%= @parteobra.gruposdetrabajo.subsectore.sectore.descripcion %></td>    
  </tr>
  <tr>
    <td>WBS Nivel 2</td>
    <td><%= @parteobra.gruposdetrabajo.subsectore.descripcion %></td>
  </tr>
  <tr>
    <td>Fecha de Ingreso</td>
    <td><%= @parteobra.fecha.strftime("%d %b. %Y") %></td>
  </tr>
</table>




        <h2>Produccion</h2>
        <table class="table table-bordered table-striped table-condensed table-hover" style="font-size:11px;">
          <tr>
            <th>#id</th>
            <th>Partidas</th>
            <th>Unidad</th>
            <th>Cantidad</th>
            <th>P.U.</th>
            <th>Observacion</th>
          </tr>
          <% @parteobra.partidas.each do |partida| %>
          <tr>
            <td><%= partida.id %></td>
            <td><%= partida.lista.nombre %></td>
            <td><%= partida.unidade.nombre %></td>
            <td><%= partida.metrado %></td>
            <td><%= partida.lista.precio_unitario %></td>
            <td><%= partida.observacion %></td>
          </tr>
          <% end %>
        </table> 


<div class="form-actions">  
  <%= link_to raw('<i class="icon-arrow-left icon-white"></i> Regresar'), parteobras_path, :class => "btn btn-primary" %>  
</div>


