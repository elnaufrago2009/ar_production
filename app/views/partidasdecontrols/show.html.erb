<p id="notice"><%= notice %></p>

<div class="page-header">
	<h2>Detalle Partida de Control</h2>
</div>

<div class="row">
<table class="table table-striped table-bordered table-condensed table-hover span6">
	<tr>
		<th>Nombre</th>
		<th>Descripcion</th>
	</tr>
	<tr>
		<td>#id</td>
		<td><%= @partidasdecontrol.id %></td>
	</tr>	
	<tr>
		<td>Codigo Subsector</td>
		<td>
			<%= @partidasdecontrol.subsectore.sectore.codigo %><%= @partidasdecontrol.subsectore.codigo %>
		</td>
	</tr>
	<tr>
		<td>WBS Nivel 1</td>
		<td><%= @partidasdecontrol.subsectore.sectore.descripcion %></td>
	</tr>
	<tr>
		<td>WBS Nivel 2</td>
		<td><%= @partidasdecontrol.subsectore.descripcion %></td>
	</tr>
	<tr>
		<td>Codigo SubFase</td>
		<td>
			<%= @partidasdecontrol.subfase.fase.codigo %><%= @partidasdecontrol.subfase.codigo %>
		</td>
	</tr>
	<tr>
		<td>Fase</td>
		<td><%= @partidasdecontrol.subfase.fase.descripcion %></td>
	</tr>
	<tr>
		<td>Sub Fase</td>
		<td><%= @partidasdecontrol.subfase.descripcion %></td>
	</tr>
</table>
</div>
<div class="clearfix"></div> 
<div class="form-actions">
	<%= link_to raw('<i class="icon-edit icon-white"></i> Edit'), edit_partidasdecontrol_path(@partidasdecontrol), :class => "btn btn-success" %>
	<%= link_to raw('<i class="icon-arrow-left icon-white"></i> Regresar'), partidasdecontrols_path, :class => "btn btn-primary" %>	
</div>

