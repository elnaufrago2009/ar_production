<p id="notice"><%= notice %></p>
<div class="page-header">
	<h2>Detalle Tipo de Combustible</h2>
</div>


<div class="row">
	<table class="table table-striped table-bordered table-condensed span4">	
		<tr>		
			<th>Nombre</th>
			<th>Descripcion</th>
		</tr>	
		<tr>
			<td>#id</td>
			<td><%= @combustibletipo.id %></td>
		</tr>
		<tr>
			<td>Tipo</td>
			<td><%= @combustibletipo.tipo %></td>
		</tr>
		<tr>
			<td>Combustible</td>
			<td><%= @combustibletipo.combustible %></td>
		</tr>
		<tr>
			<td>Precio</td>
			<td><%= @combustibletipo.precio %></td>
		</tr>
	</table>
</div>


<div class="form-actions">
	<%= link_to raw("<i class='icon-edit icon-white'></i> Editar"), edit_combustibletipo_path(@combustibletipo), class: 'btn btn-success' %>
	<%= link_to raw("<i class='icon-arrow-left icon-white'></i> Regresar"), combustibletipos_path, class: 'btn btn-primary' %>	
</div>


