<h1>Listing semanas</h1>

<table>
  <tr>
    <th>Numero</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @semanas.each do |semana| %>
  <tr>
    <td><%= semana.numero %></td>
    <td><%= link_to 'Show', semana %></td>
    <td><%= link_to 'Edit', edit_semana_path(semana) %></td>
    <td><%= link_to 'Destroy', semana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Semana', new_semana_path %>
